pipeline
{
tools{
    maven 'maven'
   

}
    agent {

    label 'node1'

     }
    stages{
        stage('build'){
            steps{
                sh 'mvn clean package'
            }
            post {
                success {
                    archiveArtifacts artifacts: '**/target/*.jar'
                }
            }
        }
    }
}
