pipeline
{
tools{
    maven 'maven'
}
environment{
    name = 'dibya'
}
parameters{
    string defaultValue: 'barwa', name: 'lastname'
}

agent {

label 'node1'

    }
    stages{
        stage('build'){
            steps{
                sh 'mvn clean package'
                echo "Hello $name ${params.name}"
            }
            post {
                success {
                    archiveArtifacts artifacts: '**/target/*.jar'
                }
            }
        }
    }
}
