pipeline
{
tools{
    maven 'maven'
}
    agent {

  l   label 'node1'

     }
    stages{
        stage('build'){
            steps{
                sh 'mvn clean package'
            }
            post{
                success {
                    archiveArtifacts artifacts: '**/target/*.war'
                }
            }
        }
    }
}
